  // SETTINGS SINGLETON EXAMPLE
  //
  "email": {
    "$description": "Description can be passed for editor",
    "type": "text",
    "id": "placeholder",
    "label": "Placeholder",
    "info": "The placeholder (label) value",
    "default": "Email Address"
  },
  //-----------------------------------------------------
