{
  "syncify": {
    "stores": {},
    "config": {
      "clean": true,
      "input": "source",
      "export": "export",
      "editor": "vscode",
      "import": "import",
      "output": "theme",
      "paths": {
        "assets": "assets/**/*",
        "config": "config/*.json",
        "locales": "locales/*.json",
        "layout": "layout/*.liquid",
        "schema": "schema/*.json",
        "blocks": ["blocks/*.liquid"],
        "templates": "templates/*",
        "customers": "customers/*",
        "metaobject": "metaobject/*",
        "metafields": "metafields/**/*.json",
        "pages": "pages/**/*",
        "redirects": "redirects.yaml",
        "snippets": {
          "[name].[dir]": ["."]
        },
        "sections": {
          "[dir]-[name]": [".s"]
        }
      },
      "processor": {},
      "views": {
        "sections": {
          "prefixDir": false,
          "separator": "-",
          "global": []
        },
        "snippets": {
          "prefixDir": false,
          "separator": "-",
          "global": []
        },
        "pages": {
          "language": "markdown",
          "suffixDir": false,
          "author": "",
          "global": []
        }
      },
      "hot": {
        "label": "visible",
        "inject": true,
        "server": 3000,
        "socket": 8089,
        "method": "hot",
        "strategy": "hydrate",
        "scroll": "preserved",
        "layouts": ["theme.liquid"]
      },
      "log": {
        "stats": true,
        "warnings": true,
        "silent": false,
        "clear": true
      },
      "transform": {
        "script": {
          "assets/[file]": "assets/scripts/*.js"
        },
        "style": {
          "assets/[file]": "assets/styles/*.css"
        },
        "json": {
          "sortExclude": []
        }
      },
      "svg": {
        "assets/foo.": ""
      }
    }
  }
}
