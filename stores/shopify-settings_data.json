{
  "title": "JSON Schema for Shopify Settings Schema",
  "$schema": "http://json-schema.org/draft-07/schema",
  "description": "Settings data files used by Shopify theme",
  "type": "object",
  "additionalProperties": true,
  "required": ["current", "presets"],
  "properties": {
    "current": {
      "type": "string",
      "description": "The current theme settings name"
    },
    "presets": {
      "type": "object",
      "additionalProperties": true,
      "description": "Presets which belong to the theme"
    }
  }
}
